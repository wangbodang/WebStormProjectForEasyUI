        <script>
        $(function(){
            $('#dataTable').on('dblclick','td',function(){
                //console.info($(this).text());
                var oldVal = $(this).text();
                var input = "<input style='width:90px;height:25px;margin:0px;padding:0px;' type='text' id='tmpId' value='" + oldVal + "' >";
                $(this).text('');
                $(this).append(input);
                $('#tmpId').focus();
                $('#tmpId').blur(function(){
                    if($(this).val() != ''){
                        oldVal = $(this).val();
                    }
                    //closest：是从当前元素开始，沿Dom树向上遍历直到找到已应用选择器的一个匹配为止。
                    $(this).closest('td').text(oldVal);
                });
            });
        });
